<div class="sg-colors">
    <div class="sg-group">
        <p class="c-secondary fs-sm">{{color | upper}}</p>
        <hr/>
    </div>
    {% for val in shade %}
    <div class="sg-color">
        <div class="sg-color__chip bg-color-{{color}}-{{val}}"></div>
        <div class="sg-color__info">
            <span><strong>SCSS:</strong> material-color('{{color}}', '{{val}}')</span>
        </div>
    </div>
    {% endfor %}
</div>