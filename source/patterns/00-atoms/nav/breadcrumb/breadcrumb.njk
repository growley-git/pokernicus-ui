<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        {% for item in items %}
        <li class="breadcrumb-item{% if loop.last %} active{% endif %}"{% if loop.last %} aria-current="page"{% endif %}>
            {% if not loop.last %}
            <a href="#">{{item.text}}</a>
            {% else %}
            {{item.text}}
            {% endif %}
        </li>
        {% endfor %}
    </ol>
</nav>